ALGORITHM dot_product
VAR
    tabV1()= ARRAY_[N] REEL
    tabV2()= ARRAY_[N] REEL
    i = INTEGER
    ps = REEL
    ps := 0
BEGIN
    FOR i FROM 1 TO N-1 DO 
    ps := ps + V1[i] * V2[i]
    DO 
    return : ps
END     
